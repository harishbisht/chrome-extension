 {
    "name": "Absolutdata Office Hour App",
    "version": "1.0",
    "description": "Office Hour Apps",
    "manifest_version": 2,
    "browser_action":{
    	"default_popup":"popup.html"
    },
    "permissions": [
				    "storage",
            "activeTab",
				    "https://*.herokuapp.com/*"
					],
    "background": {
       "scripts": [
         "js/jquery.min.js",
         "js/bootstrap.min.js",
         "js/script.js"
       ],
       "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}